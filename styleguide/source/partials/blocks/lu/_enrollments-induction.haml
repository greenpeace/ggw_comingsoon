.block#block-enrollments
  %i.block-icon.icon-users.icon-bg-event
  %h2.block-title{"data-amtitle" => "Enrollments"} Enrollments

  .content
    .members
      - 1.times do |num|
        %a.member-item{:href => "profile-alen.html"}
          %img.lazyload{:alt => "Alen Marsh", :src => "/images/blank.png", "data-srcset" => "/photos/75/alen.jpg"}/
          %noscript
            %img{:alt => "Name of user", :src => "/photos/75/alen.jpg"}/
        %a.member-item{:href => "./profile.html"}
          %img.lazyload{:alt => "Name of user", :src => "/images/blank.png", "data-srcset" => "/photos/75/member3.jpg"}/
          %noscript
            %img{:alt => "Name of user", :src => "/photos/75/member3.jpg"}/
        %a.member-item{:href => "./profile.html"}
          %img.lazyload{:alt => "Name of user", :src => "/images/blank.png", "data-srcset" => "/photos/75/member4.jpg"}/
          %noscript
            %img{:alt => "Name of user", :src => "/photos/75/member4.jpg"}/
        %a.member-item{:href => "./profile.html"}
          %img.lazyload{:alt => "Name of user", :src => "/images/blank.png", "data-srcset" => "/photos/75/member5.jpg"}/
          %noscript
            %img{:alt => "Name of user", :src => "/photos/75/member5.jpg"}/
        %a.member-item{:href => "./profile.html"}
          %img.lazyload{:alt => "Name of user", :src => "/images/blank.png", "data-srcset" => "/photos/75/member6.jpg"}/
          %noscript
            %img{:alt => "Name of user", :src => "/photos/75/member6.jpg"}/
    %p 5 persons are going

    - if current_page.data.organiser == "organiser"
      %a.button{:href => "./event-manage-enrollments.html"} Manage enrollments
    - else
      %a.button{:href => "./event-view-enrollments.html"} View enrollments
