#block-members.block
  %i.block-icon.icon-bg-group.icon-users
  %h2.block-title
    Members of
    = current_page.data.title
  .content
    .members
      - if current_page.data.user_role == "lu"
        - 14.times do |num|
          - if num  > 1
            .member-item
              %a.member-item{:href => "./profile.html"}
                %img.lazyload{:alt => "Name of user", :src => "data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==", "data-srcset" => "/photos/75/member#{num}.jpg"}/
                %noscript
                  %img{:alt => "Name of user", :src => "/photos/75/member#{num}.jpg"}/

      - else

        - 14.times do |num|
          - if num  > 1
            .member-item
              %img.lazyload{:alt => "Name of user", :src => "data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==", "data-srcset" => "/photos/75/member#{num}.jpg"}/
              %noscript
                %img{:alt => "Name of user", :src => "/photos/75/member#{num}.jpg"}/
