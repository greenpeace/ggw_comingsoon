#block-user-activity.block.lazyload{"data-expand" => "-40"}
  %i.block-icon.icon-bg-user.icon-clock
  %h2.block-title{"data-amtitle" => "Activity"} Activity log
  .content
    .view-ggw-user-activity
      .view-content

        - if current_page.data.access == "mine"

          - data.members.member_one.each do |item|
            .views-row
              .views-field.views-field-timestamp
                16 August 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  created
                  %a{:href => "#"} Kom wachtlopen aan boord van de Arctic Sunrise
            .views-row
              .views-field.views-field-timestamp
                14 July 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  created
                  %a{:href => "#"} Windenergy
            .views-row
              .views-field.views-field-timestamp
                1 May 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  went to
                  %a{:href => "#"} Debating on solar energy solutions
                %p
                  #{item.first_name} #{item.last_name}
                  replied on
                  %a{:href => "#"} Forests in Europe
            .views-row
              .views-field.views-field-timestamp
                29 April 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  replied on
                  %a{:href => "#"} Why bother?

        - else

          - data.members.member_two.each do |item|
            .views-row
              .views-field.views-field-timestamp
                16 August 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  created
                  %a{:href => "#"} Kom wachtlopen aan boord van de Arctic Sunrise
            .views-row
              .views-field.views-field-timestamp
                14 July 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  created
                  %a{:href => "#"} Windenergy
            .views-row
              .views-field.views-field-timestamp
                1 May 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  went to
                  %a{:href => "#"} Debating on solar energy solutions
                %p
                  #{item.first_name} #{item.last_name}
                  replied on
                  %a{:href => "#"} Forests in Europe
            .views-row
              .views-field.views-field-timestamp
                29 April 2014
              .views-field.views-field-rendered-entity
                %p
                  #{item.first_name} #{item.last_name}
                  replied on
                  %a{:href => "#"} Why bother?

    %a.button{:href => "#"} load more
