- data.groups.group_country.each do |item|
  .views-row
    %article.node-teaser-group.node-teaser
      .field-type-image
      .field-type-wrapper
        %h2.node-title
          %a{:href => "./group-home-countrygroup.html"}
            = item.title
        .submitted
          %a.type.group{:href => "./groups.html"} Group
          #{item.group_type} (#{item.enrollment_type})
        .field-name-body
          = item.about
